    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@mojotech/json-type-validation": "^3.1.0",
    "app-root-path": "^3.0.0",
    "clone-deep": "^4.0.1",
    "fast-deep-equal": "^3.1.3",
    "luxon": "^1.26.0",
    "moment": "^2.29.1",
