{
  "name": "ystd",
  "version": "1.0.9",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "inprint & npm run lint && npm run build:clean && npm run build:ts && npm run build:docs",
    "build:ts": "tsc",
    "build_old:ts": "npx rollup -c",
    "build:docs": "api-extractor run --local && api-documenter markdown --input-folder temp --output-folder docs",
    "build:clean": "rm -rf ./dist/*",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "tsc": "tsc",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "republish": "npm run build && npm publish"
  },
  "author": "Yuri Yaryshev",
  "homepage": "https://github.com/yuyaryshev/ystd",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuyaryshev/ystd"
  },
  "license": "Unlicense",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@microsoft/api-documenter": "^7.13.5",
    "@microsoft/api-extractor": "^7.15.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/react": "^11.2.6",
    "@types/chai": "^4.2.17",
    "@types/jest": "^26.0.23",
    "@types/luxon": "^1.26.5",
    "@types/react": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^33.0.0",
    "eslint-plugin-sonarjs": "^0.7.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "javascript-stringify": "^2.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup": "^2.45.2",
    "ts-jest": "^26.5.5",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
